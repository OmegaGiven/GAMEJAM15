config_version=5

[application]

config/name="GAMEJAM15"
run/main_scene="res://scenes/Menu Scenes/MainMenu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GamePaths="*res://global scripts/GamePaths.gd"
MultiplayerControls="*res://global scripts/MultiplayerControls.gd"
